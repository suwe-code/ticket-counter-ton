{
    "name": "ton-source-map",
    "version": "0.2.2",
    "description": "TypeScript type definitions and functions for TON Source Map format used by Tolk and FunC compilers",
    "keywords": [
        "ton",
        "tolk",
        "func",
        "tvm",
        "source-map"
    ],
    "homepage": "https://github.com/i582/ton-source-map#readme",
    "bugs": {
        "url": "https://github.com/i582/ton-source-map/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/i582/ton-source-map.git"
    },
    "license": "MIT",
    "author": "TON Core",
    "type": "commonjs",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist/",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=18.0.0"
    },
    "sideEffects": false,
    "scripts": {
        "build": "tsc --declaration",
        "test": "jest",
        "fmt": "prettier --write -l --cache .",
        "fmt:check": "prettier --check --cache .",
        "precommit": "yarn build && yarn fmt && yarn test",
        "prepare": "yarn precommit"
    },
    "devDependencies": {
        "@ton/core": "^0.60.1",
        "@ton/crypto": "^3.3.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.30",
        "jest": "^29.7.0",
        "prettier": "^3.5.3",
        "ton-assembly": "^0.3.0",
        "ts-jest": "^29.3.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": false,
        "printWidth": 100,
        "semi": false,
        "singleQuote": false,
        "tabWidth": 4,
        "trailingComma": "all",
        "useTabs": false,
        "overrides": [
            {
                "files": [
                    "*.yaml",
                    "*.yml"
                ],
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "yarn@4.6.0"
}
